{
  "name": "@psycle/repsycle",
  "version": "0.0.2",
  "description": "Psycle Research front-end toolkit",
  "author": "Psycle Research",
  "keywords": [
    "psycle",
    "repsycle"
  ],
  "license": "MIT",
